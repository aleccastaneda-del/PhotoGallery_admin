	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Debug prod",
			"skipFiles": ["<node_internals>/**"],
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run-script","debug-prod"],
			"port": 9229
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Debug dev-local",
			"skipFiles": ["<node_internals>/**"],
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run-script","debug-dev-local"],
			"port": 9229
		}
	]
}
